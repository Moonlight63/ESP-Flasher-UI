{
  "name": "iot-stadium-flasher",
  "version": "0.1.0",
  "description": "A simple tool to flash IoT Stadium ESP based products.",
  "main": "build/main/main.js",
  "scripts": {
    "dev": "node scripts/dev-server.js",
    "devtools": "./node_modules/.bin/vue-devtools",
    "build": "node scripts/build.js && electron-builder",
    "build:win": "node scripts/build.js && electron-builder --win",
    "build:mac": "node scripts/build.js && electron-builder --mac",
    "build:linux": "node scripts/build.js && electron-builder --linux",
    "build:pi": "node scripts/build.js && electron-builder --linux deb --arm64"
  },
  "repository": "https://github.com/Moonlight63/ESP-Flasher-UI",
  "author": {
    "name": "Dalen Catt",
    "url": "https://github.com/Moonlight63/ESP-Flasher-UI.git",
    "email": "dalen@wowrack.com"
  },
  "devDependencies": {
    "@iconify/vue": "^4.0.0",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vue/cli": "^5.0.1",
    "@vue/devtools": "^6.4.3",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "electron": "^20.1.0",
    "electron-builder": "^23.3.3",
    "postcss": "^8.4.17",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.8.2",
    "unplugin-auto-import": "^0.11.2",
    "unplugin-vue-components": "^0.22.7",
    "vite": "^3.0.9",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-pages": "^0.26.0"
  },
  "dependencies": {
    "@vueuse/electron": "^9.3.0",
    "daisyui": "^2.31.0",
    "electron-devtools-installer": "^3.2.0",
    "fs": "^0.0.1-security",
    "immer": "^9.0.15",
    "path": "^0.12.7",
    "pinia": "^2.0.22",
    "serialport": "^10.4.0",
    "vue": "^3.2.36",
    "vue-router": "^4.1.5",
    "xterm": "^5.0.0",
    "xterm-addon-fit": "^0.6.0"
  }
}
